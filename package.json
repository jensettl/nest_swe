    "re2": "1.16.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.3.1",
    "strip-indent": "3.0.0",
    "swagger-ui-express": "4.1.6"
  },
  "devDependencies": {
    "@asciidoctor/reveal.js": "5.0.0-rc.1",
    "@jest/globals": "27.2.4",
    "@jest/types": "27.2.4",
    "@nestjs/cli": "8.1.2",
    "@nestjs/schematics": "8.0.3",
    "@nestjs/testing": "8.0.10",
    "@types/bcrypt": "5.0.0",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.13",
    "@types/fs-extra": "9.0.13",
    "@types/ip": "1.1.0",
    "@types/jest": "27.0.2",
    "@types/jsonwebtoken": "8.5.5",
    "@types/multer": "1.4.7",
    "@types/node": "16.10.3",
    "@types/nodemailer": "6.4.4",
    "@types/nodemailer-direct-transport": "1.0.32",
    "@types/nodemailer-smtp-transport": "2.7.5",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "1.0.34",
    "@types/pino": "6.3.11",
    "@types/shelljs": "0.8.9",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "apollo-server-types": "3.2.0",
    "asciidoctor": "2.2.5",
    "asciidoctor-kroki": "0.14.0",
    "concurrently": "6.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-clean-regex": "0.5.1",
    "eslint-plugin-deprecation": "1.2.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.5.2",
    "eslint-plugin-jest-formatting": "3.0.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-security-node": "1.0.14",
    "eslint-plugin-unicorn": "36.0.0",
    "jest": "27.2.4",
    "jest-each": "27.2.4",
    "leasot": "12.0.0",
    "mongoose": "6.0.9",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "shelljs": "0.8.4",
    "ts-jest": "27.0.5",
    "ts-loader": "9.2.6",
    "ts-node": "10.2.1",
