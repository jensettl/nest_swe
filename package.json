{
  "name": "buch",
  "version": "1.0.0",
  "description": "Beispiel",
  "author": "Juergen Zimmermann",
  "private": true,
  "license": "GPL-3.0",
  "main": "dist/src/main.js",
  "engines": {
    "node": ">=16.10.0",
    "npm": ">=7.24.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "node dist/src/main.js",
    "prestart:dev": "npm run copy-resources",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test:ORIG": "jest",
    "pretest": "npm run copy-resources",
    "test": "set LOG_DIR=log && set DB_POPULATE_FILES=false && jest --runInBand --errorOnDeprecated --forceExit --expand",
    "test:watch": "jest --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "asciidoc": "node scripts/asciidoc",
    "clinic": "clinic doctor -- node dist/src/main.js",
    "clean": "rimraf dist log/server",
    "depcheck": "depcheck",
    "deploy:heroku": "git push heroku main",
    "deploy:prepare": "concurrently --prefix \"[{name}]\" --names \"ESLint,copy-resources,TypeScript\" --prefix-colors \"yellow.bold,gray.bold,cyan.bold,green.bold\" \"npm run eslint\" \"npm run copy-resources\" \"npm run tsc\"",
    "eslint": "eslint --ext .ts ./src ./__test__",
    "copy-resources": "node scripts/copy-resources",
    "leasot": "leasot --exit-nicely src/**/*.ts",
    "heroku:open": "heroku open --app protected-eyrie-38026 /api/000000000000000000000001",
    "heroku:open:file": "heroku open --app protected-eyrie-38026 /curl --include --tlsv1.2 --insecure https://protected-eyrie-38026.herokuapp.com/file/000000000000000000000001",
    "heroku:curl": "curl --include --tlsv1.2 --insecure https://protected-eyrie-38026.herokuapp.com/api/000000000000000000000001",
    "prepack": "npm run build && npm run copy-resources",
    "pack": "pack build juergenzimmermann/buch:1.0.0 --builder paketobuildpacks/builder:base",
    "pack:inspect": "pack inspect juergenzimmermann/buch:1.0.0",
    "prettier": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "revealjs": "asciidoctor-revealjs extras/doc/folien/folien.adoc",
    "snyk:auth": "echo https://app.snyk.io/account && snyk auth",
    "snyk": "snyk test",
    "typedoc": "typedoc",
    "tsc": "tsc --pretty",
    "tsc:watch": "npm run tsc -- --watch",
    "heroku-postbuild": "tsc --version && tsc && node scripts/copy-resources no-tls"
  },
  "keywords": [
    "Node",
    "npm",
    "Nest",
    "REST",
    "GraphQL",
    "Apollo",
    "Axios",
    "TypeScript",
    "ESLint",
    "MongoDB",
    "mongoose",
    "JWT",
    "Passport",
    "bcrypt",
    "Ajv",
    "CSP",
    "Clickjacking",
    "CORS",
    "winston",
    "nodemailer",
    "Jest",
    "ts-jest"
  ],
  "dependencies": {
    "@js-temporal/polyfill": "0.2.0",
    "@nestjs/axios": "0.0.2",
    "@nestjs/common": "8.0.10",
    "@nestjs/core": "8.0.10",
    "@nestjs/graphql": "9.0.5",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/mongoose": "9.0.0",
    "@nestjs/passport": "8.0.1",
    "@nestjs/platform-express": "8.0.10",
    "@nestjs/swagger": "5.1.0",
    "@nestjs/terminus": "8.0.1",
    "ajv": "8.6.3",
    "ajv-errors": "3.0.0",
    "ajv-formats": "2.1.1",
    "apollo-server-express": "3.3.0",
    "axios": "0.22.0",
    "bcrypt": "5.0.1",
    "bson": "4.5.3",
    "chalk": "4.1.2",
    "compression": "1.7.4",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "file-type": "16.5.3",
    "form-data": "4.0.0",
    "fs-extra": "10.0.0",
    "graphql": "15.6.1",
    "helmet": "4.6.0",
    "into-stream": "6.0.0",
    "ip": "1.1.5",
    "json5": "2.2.0",
    "jsonwebtoken": "8.5.1",
    "minimist": "1.2.5",
    "mongodb": "4.1.3",
    "nodemailer": "6.6.5",
    "os-name": "4.0.1",
    "passport": "0.5.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pino": "6.13.3",
